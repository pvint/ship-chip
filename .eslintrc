{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "module": true
  },
  "extends": [
    "eslint:recommended",
    "ember"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "plugins": [
    "eslint-plugin-ember"
  ],
  "rules": {
    "indent": [
      "warn",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "warn",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}
